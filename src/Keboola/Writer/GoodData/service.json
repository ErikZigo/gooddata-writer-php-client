{
    "name": "Keboola GoodData Writer",
    "apiVersion": "2012-11-26",
    "baseUrl": "https://gooddata-writer.keboola.com",
    "description": "Keboola GoodData Writer API PHP Client",
    "operations": {
        "UploadProject": {
            "httpMethod": "POST",
            "uri": "/upload-project",
            "summary": "Upload whole project to GoodData",
            "responseClass": "primitive",
            "parameters": {
                "writerId": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "incrementalLoad": {
                    "location": "json",
                    "type": "integer",
                    "required": false
                },
                "sanitize": {
                    "location": "json",
                    "type": "integer",
                    "required": false
                }
            }
        },
        "UploadTable": {
            "httpMethod": "POST",
            "uri": "/upload-table",
            "summary": "Upload table to GoodData",
            "responseClass": "primitive",
            "parameters": {
                "table": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "writerId": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "incrementalLoad": {
                    "location": "json",
                    "type": "integer",
                    "required": false
                },
                "sanitize": {
                    "location": "json",
                    "type": "integer",
                    "required": false
                }
            }
        },
        "Xml": {
            "httpMethod": "GET",
            "uri": "/xml",
            "summary": "Get configuration xml of selected table",
            "responseType": "primitive",
            "parameters": {
                "table": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                }
            }
        },
        "DeleteWriter": {
            "httpMethod": "DELETE",
            "uri": "/delete-writer",
            "summary": "Removes writer configuration from Storage API and schedules removal of GoodData project",
            "responseType": "primitive",
            "parameters": {
                "writerId": {
                    "location": "json",
                    "type": "string",
                    "required": true
                }
            }
        },
        "JobsList": {
            "httpMethod": "GET",
            "uri": "/jobs",
            "summary": "Get jobs list",
            "responseType": "primitive",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "count": {
                    "location": "query",
                    "type": "integer",
                    "required": false
                },
                "offset": {
                    "location": "query",
                    "type": "integer",
                    "required": false
                }
            }
        },
        "BatchStatus": {
            "httpMethod": "GET",
            "uri": "/batch/{batch}",
            "summary": "Get jobs batch status",
            "responseType": "primitive",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "batch": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                }
            }
        },
        "JobStatus": {
            "httpMethod": "GET",
            "uri": "/job/{job}",
            "summary": "Get job status",
            "responseType": "primitive",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "job": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                }
            }
        },
        "JobXml": {
            "httpMethod": "GET",
            "uri": "/job/{job}",
            "summary": "Get job status",
            "responseType": "primitive",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "job": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                },
                "detail": {
                    "location": "query",
                    "type": "string",
                    "required": true,
                    "default": "xml",
                    "static": true
                }
            }
        },
        "JobCsv": {
            "httpMethod": "GET",
            "uri": "/job/{job}",
            "summary": "Get job status",
            "responseType": "primitive",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "job": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                },
                "detail": {
                    "location": "query",
                    "type": "string",
                    "required": true,
                    "default": "csv",
                    "static": true
                }
            }
        }
    }
}