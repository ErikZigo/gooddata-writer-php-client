{
    "name": "Keboola GoodData Writer",
    "apiVersion": "2012-11-26",
    "baseUrl": "https://gooddata-writer.keboola.com",
    "description": "Keboola GoodData Writer API PHP Client",
    "operations": {
        "UploadProject": {
            "httpMethod": "POST",
            "uri": "/upload-project",
            "summary": "Upload whole project to GoodData",
            "responseClass": "UploadOutput",
            "parameters": {
                "writerId": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "incrementalLoad": {
                    "location": "json",
                    "type": "bool",
                    "required": false,
                    "default": false
                },
                "sanitize": {
                    "location": "json",
                    "type": "bool",
                    "required": false,
                    "default": true
                }
            }
        },
        "UploadTable": {
            "httpMethod": "POST",
            "uri": "/upload-table",
            "summary": "Upload table to GoodData",
            "responseClass": "UploadOutput",
            "parameters": {
                "table": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "writerId": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "incrementalLoad": {
                    "location": "json",
                    "type": "bool",
                    "required": false,
                    "default": false
                },
                "sanitize": {
                    "location": "json",
                    "type": "bool",
                    "required": false,
                    "default": true
                }
            }
        },
        "Xml": {
            "httpMethod": "GET",
            "uri": "/xml?table={table}&writerId={writerId}",
            "summary": "Get configuration xml of selected table",
            "responseType": "primitive",
            "parameters": {
                "table": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                }
            }
        },
        "DeleteWriter": {
            "httpMethod": "DELETE",
            "uri": "/delete-writer",
            "summary": "Removes writer configuration from Storage API and schedules removal of GoodData project",
            "responseType": "BasicOutput",
            "parameters": {
                "writerId": {
                    "location": "json",
                    "type": "string",
                    "required": true
                }
            }
        },
        "JobsList": {
            "httpMethod": "GET",
            "uri": "/jobs?writerId={writerId}&count={count}&offset={offset}",
            "summary": "Get jobs list",
            "responseType": "JobsListOutput",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "count": {
                    "location": "query",
                    "type": "int",
                    "required": false
                },
                "offset": {
                    "location": "query",
                    "type": "int",
                    "required": false
                }
            }
        },
        "BatchStatus": {
            "httpMethod": "GET",
            "uri": "/batch/{batch}?writerId={writerId}",
            "summary": "Get jobs batch status",
            "responseType": "BatchStatusOutput",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "batch": {
                    "location": "query",
                    "type": "string",
                    "required": true
                }
            }
        },
        "JobStatus": {
            "httpMethod": "GET",
            "uri": "/job/{job}?writerId={writerId}",
            "summary": "Get job status",
            "responseType": "JobStatusOutput",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "job": {
                    "location": "query",
                    "type": "int",
                    "required": true
                }
            }
        },
        "JobXml": {
            "httpMethod": "GET",
            "uri": "/job/{job}?writerId={writerId}&detail=xml",
            "summary": "Get job status",
            "responseType": "primitive",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "job": {
                    "location": "query",
                    "type": "int",
                    "required": true
                },
                "detail": {
                    "location": "query",
                    "type": "string",
                    "required": true,
                    "default": "xml",
                    "static": true
                }
            }
        },
        "JobCsv": {
            "httpMethod": "GET",
            "uri": "/job/{job}?writerId={writerId}&detail=csv",
            "summary": "Get job status",
            "responseType": "primitive",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "job": {
                    "location": "query",
                    "type": "int",
                    "required": true
                },
                "detail": {
                    "location": "query",
                    "type": "string",
                    "required": true,
                    "default": "csv",
                    "static": true
                }
            }
        }
    },
    "models": {
        "BasicOutput": {
            "type": "object",
            "properties": {
                "status": {
                    "location": "json",
                    "type": "string"
                }
            }
        },
        "UploadOutput": {
            "type": "object",
            "properties": {
                "status": {
                    "location": "json",
                    "type": "string"
                },
                "batch": {
                    "location": "json",
                    "type": "string"
                },
                "jobs": {
                    "location": "json",
                    "type": "array"
                }
            }
        },
        "JobsListOutput": {
            "type": "object",
            "properties": {
                "status": {
                    "location": "json",
                    "type": "string"
                },
                "jobs": {
                    "location": "json",
                    "type": "array"
                }
            }
        },
        "BatchStatusOutput": {
            "type": "object",
            "properties": {
                "status": {
                    "location": "json",
                    "type": "string"
                },
                "createdTime": {
                    "location": "json",
                    "type": "string"
                },
                "jobs": {
                    "location": "json",
                    "type": "array"
                }
            }
        },
        "JobStatusOutput": {
            "type": "object",
            "properties": {
                "status": {
                    "location": "json",
                    "type": "string"
                },
                "id": {
                    "location": "json",
                    "type": "int"
                },
                "idBatch": {
                    "location": "json",
                    "type": "int"
                },
                "command": {
                    "location": "json",
                    "type": "string"
                },
                "dataset": {
                    "location": "json",
                    "type": "string"
                },
                "createdTime": {
                    "location": "json",
                    "type": "string"
                },
                "finishedTime": {
                    "location": "json",
                    "type": "string"
                },
                "jobStatus": {
                    "location": "json",
                    "type": "string"
                },
                "logFile": {
                    "location": "json",
                    "type": "string"
                },
                "outputFile": {
                    "location": "json",
                    "type": "string"
                }
            }
        }
    }
}