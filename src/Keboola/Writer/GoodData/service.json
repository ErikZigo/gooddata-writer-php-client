{
    "name": "Keboola GoodData Writer",
    "apiVersion": "2013-10-14 (v 4.6)",
    "baseUrl": "https://syrup.keboola.com/gooddata-writer",
    "description": "Keboola GoodData Writer API PHP Client",
    "operations": {
		"GetWriters": {
			"httpMethod": "GET",
			"uri": "writers",
			"summary": "Get list of configured writers"
		},
		"CreateWriter": {
			"httpMethod": "POST",
			"uri": "writers",
			"summary": "Create writer",
			"parameters": {
				"writerId": {
					"location": "json",
					"type": "string",
					"required": true
				},
				"wait": {
					"location": "json",
					"type": "integer",
					"required": false
				}
			}
		},
		"DeleteWriter": {
			"httpMethod": "DELETE",
			"uri": "writers?writerId={writerId}",
			"summary": "Delete writer",
			"parameters": {
				"writerId": {
					"location": "uri",
					"type": "string",
					"required": true
				}
			}
		},
        "UploadProject": {
            "httpMethod": "POST",
            "uri": "upload-project",
            "summary": "Upload whole project to GoodData",
            "parameters": {
                "writerId": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
				"incrementalLoad": {
					"location": "json",
					"type": "integer",
					"required": false
				}
            }
        },
        "UploadTable": {
            "httpMethod": "POST",
            "uri": "upload-table",
            "summary": "Upload table to GoodData",
            "parameters": {
                "table": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "writerId": {
                    "location": "json",
                    "type": "string",
                    "required": true
                },
                "incrementalLoad": {
                    "location": "json",
                    "type": "integer",
                    "required": false
                }
            }
        },
        "Xml": {
            "httpMethod": "GET",
            "uri": "xml",
            "summary": "Get configuration xml of selected table",
            "parameters": {
                "table": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                }
            }
        },
        "JobsList": {
            "httpMethod": "GET",
            "uri": "jobs",
            "summary": "Get jobs list",
            "parameters": {
                "writerId": {
                    "location": "query",
                    "type": "string",
                    "required": true
                }
            }
        },
        "BatchStatus": {
            "httpMethod": "GET",
            "uri": "batch?writerId={writerId}&batchId={batchId}",
            "summary": "Get jobs batch status",
            "parameters": {
                "writerId": {
                    "location": "uri",
                    "type": "string",
                    "required": true
                },
                "batchId": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                }
            }
        },
        "JobStatus": {
            "httpMethod": "GET",
            "uri": "jobs?writerId={writerId}&jobId={jobId}",
            "summary": "Get job status",
            "parameters": {
                "writerId": {
                    "location": "uri",
                    "type": "string",
                    "required": true
                },
                "jobId": {
                    "location": "uri",
                    "type": "integer",
                    "required": true
                }
            }
        }
    }
}